		</div>
	</header>

	<section id="experiences">
		<h1>Expériences</h1>
		<dl>
			{{#each experiences}}
				<dt>Du {{formatDate start 'DD/MM/YYYY'}}
					{{#if end}}Au {{formatDate end 'DD/MM/YYYY'}}{{else}}Jusqu'à aujourd'hui{{/if}}
				</dt>
				<dd>
					<p class="description"><strong>{{name}} </strong> : {{description}}</p>

					<p class="job">{{job}} :</p>
					{{#if roles}}
						<ul>
							{{#each roles}}
								<li>{{this}}</li>
							{{/each}}
						</ul>
					{{/if}}

					{{#if methods}}
						<p class="methods">Méthodologie de travail :</p>
						<ul>
							{{#each methods}}
								<li>{{this}}</li>
							{{/each}}
						</ul>
					{{/if}}
					{{#if tools}}
						<p class="tools">Outils et technologies :</p>
						<ul>
							{{#each tools}}
								<li>{{this}}</li>
							{{/each}}
						</ul>
					{{/if}}
					<p>Durée : {{{duration start end true}}}</p>
				</dd>
			{{/each}}
		</dl>
	</section>

	<section id="projects">
		<h1>Projets Personnels</h1>
		<dl>
			{{#each projects}}
				<dt>{{name}}</dt>
				<dd>
					<p>{{description}}</p>

					<p class="tools">Outils et technologies :</p>
					<ul>
						{{#each tools}}
							<li>{{this}}</li>
						{{/each}}
					</ul>
				</dd>
			{{/each}}
		</dl>
	</section>

	<section id="competences">
		<h1>Compétences</h1>
		<dl>
			{{#each competences}}
				<dt>{{name}}</dt>
				<dd>
					<ul>
						{{#each skills}}
							<li class="level-{{level}}">{{name}}</li>
						{{/each}}
					</ul>
				</dd>
			{{/each}}
		</dl>
	</section>

